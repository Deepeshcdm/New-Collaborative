/* ====================================================================
   LOGIN PAGE STYLES - Modern Arc Design with Sigmoid Curve
   ==================================================================== */

/* ============================================================
   CONTAINER & BACKGROUND
   ============================================================ */
.login-container {
    min-height: 100vh;
    display: flex;
    align-items: center;
    justify-content: center;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    padding: 20px;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    position: relative;
    overflow: hidden;
}

.login-container::before {
    content: '';
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background:
        radial-gradient(circle at 20% 50%, rgba(120, 119, 198, 0.3) 0%, transparent 50%),
        radial-gradient(circle at 80% 20%, rgba(255, 255, 255, 0.1) 0%, transparent 50%),
        radial-gradient(circle at 40% 80%, rgba(120, 119, 198, 0.2) 0%, transparent 50%);
    animation: floatingBackground 20s ease-in-out infinite;
}

@keyframes floatingBackground {

    0%,
    100% {
        transform: translate(0, 0) rotate(0deg);
    }

    33% {
        transform: translate(-20px, -10px) rotate(1deg);
    }

    66% {
        transform: translate(20px, 10px) rotate(-1deg);
    }
}

/* ============================================================
   MAIN CARD STRUCTURE
   ============================================================ */
.login-card {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(15px);
    border-radius: 24px;
    padding: 0;
    width: 100%;
    max-width: 420px;
    box-shadow:
        0 25px 50px rgba(0, 0, 0, 0.15),
        0 0 0 1px rgba(255, 255, 255, 0.2),
        inset 0 1px 0 rgba(255, 255, 255, 0.3);
    border: 1px solid rgba(255, 255, 255, 0.2);
    animation: slideUp 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    position: relative;
    z-index: 1;
    overflow: hidden;
}

/* Sigmoid/Hill Exponential Curve Arc */
.login-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 280px;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 50%, #9f7aea 100%);
    clip-path: polygon(0 0,
            100% 0,
            100% 40%,
            95% 45%,
            85% 55%,
            70% 65%,
            50% 70%,
            30% 65%,
            15% 55%,
            5% 45%,
            0 40%);
    z-index: 1;
}

@keyframes slideUp {
    from {
        opacity: 0;
        transform: translateY(40px) scale(0.95);
    }

    to {
        opacity: 1;
        transform: translateY(0) scale(1);
    }
}

/* ============================================================
   APP ICON SECTION
   ============================================================ */
.app-icon-container {
    position: absolute;
    top: 70px;
    left: 50%;
    transform: translateX(-50%);
    width: 85px;
    height: 85px;
    background: rgba(255, 255, 255, 0.95);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 2;
    box-shadow:
        0 12px 30px rgba(0, 0, 0, 0.2),
        0 0 0 3px rgba(255, 255, 255, 0.5);
    backdrop-filter: blur(15px);
    animation: iconFloat 6s ease-in-out infinite;
}

@keyframes iconFloat {

    0%,
    100% {
        transform: translateX(-50%) translateY(0px);
    }

    50% {
        transform: translateX(-50%) translateY(-3px);
    }
}

.app-icon {
    width: 55px;
    height: 55px;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 26px;
    font-weight: bold;
    box-shadow: inset 0 2px 4px rgba(255, 255, 255, 0.2);
}

/* ============================================================
   CONTENT SECTION
   ============================================================ */
.login-content {
    padding: 200px 40px 40px 40px;
    position: relative;
    z-index: 2;
}

/* ============================================================
   HEADER SECTION
   ============================================================ */
.login-header {
    text-align: center;
    margin-bottom: 30px;
}

.login-title {
    font-size: 32px;
    font-weight: 700;
    color: #2d3748;
    margin: 0 0 8px 0;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    animation: titleGlow 2s ease-in-out infinite alternate;
    text-align: center;
}

@keyframes titleGlow {
    from {
        filter: drop-shadow(0 0 5px rgba(102, 126, 234, 0.3));
        transform: scale(1);
    }

    to {
        filter: drop-shadow(0 0 10px rgba(102, 126, 234, 0.5));
        transform: scale(1.02);
    }
}

.login-subtitle {
    color: #718096;
    font-size: 14px;
    margin: 0;
    font-weight: 400;
}

/* ============================================================
   ERROR MESSAGING
   ============================================================ */
.error-message {
    background: #fed7d7;
    color: #c53030;
    padding: 12px 16px;
    border-radius: 8px;
    margin-bottom: 20px;
    font-size: 14px;
    border: 1px solid #feb2b2;
    animation: shake 0.5s ease-in-out;
}

@keyframes shake {

    0%,
    100% {
        transform: translateX(0);
    }

    25% {
        transform: translateX(-5px);
    }

    75% {
        transform: translateX(5px);
    }
}

/* ============================================================
   SOCIAL LOGIN SECTION
   ============================================================ */
.social-login-section {
    margin-bottom: 24px;
}

.social-button {
    width: 100%;
    padding: 16px 20px;
    border: none;
    border-radius: 16px;
    font-size: 15px;
    font-weight: 600;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 12px;
    margin-bottom: 12px;
    transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    position: relative;
    overflow: hidden;
    transform-origin: center;
}

.social-button::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    background: rgba(255, 255, 255, 0.2);
    border-radius: 50%;
    transform: translate(-50%, -50%);
    transition: all 0.6s ease;
}

.social-button:active::before {
    width: 300px;
    height: 300px;
}

.social-button:hover {
    transform: translateY(-3px) scale(1.02);
    box-shadow: 0 12px 25px rgba(0, 0, 0, 0.2);
}

.social-button:active {
    transform: translateY(-1px) scale(0.98);
    transition: all 0.1s ease;
}

.social-button:disabled {
    opacity: 0.6;
    cursor: not-allowed;
    transform: none;
}

/* Google Button Styling */
.google-button {
    background: linear-gradient(135deg, #fff 0%, #f8f9fa 100%);
    color: #374151;
    border: 2px solid #e5e7eb;
    position: relative;
}

.google-button::after {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.4), transparent);
    transition: left 0.6s ease;
}

.google-button:hover::after {
    left: 100%;
}

.google-button:hover:not(:disabled) {
    background: linear-gradient(135deg, #f9fafb 0%, #f3f4f6 100%);
    border-color: #d1d5db;
    transform: translateY(-3px) scale(1.02);
}

/* Facebook Button Styling */
.facebook-button {
    background: linear-gradient(135deg, #1877f2 0%, #42a5f5 100%);
    color: white;
    border: 2px solid #1877f2;
    position: relative;
}

.facebook-button::after {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
    transition: left 0.6s ease;
}

.facebook-button:hover::after {
    left: 100%;
}

.facebook-button:hover:not(:disabled) {
    background: linear-gradient(135deg, #166fe5 0%, #1976d2 100%);
    border-color: #166fe5;
    transform: translateY(-3px) scale(1.02);
}

.social-icon {
    font-size: 18px;
}

/* ============================================================
   DIVIDER SECTION
   ============================================================ */
.divider {
    display: flex;
    align-items: center;
    margin: 24px 0;
}

.divider-line {
    flex: 1;
    height: 1px;
    background: #e2e8f0;
}

.divider-text {
    padding: 0 16px;
    color: #718096;
    font-size: 14px;
    font-weight: 500;
}

/* ============================================================
   FORM SECTION
   ============================================================ */
.login-form {
    margin-bottom: 24px;
}

.input-group {
    margin-bottom: 20px;
}

.input-wrapper {
    position: relative;
    display: flex;
    align-items: center;
    animation: inputSlideIn 0.6s ease-out;
}

@keyframes inputSlideIn {
    from {
        opacity: 0;
        transform: translateX(-20px);
    }

    to {
        opacity: 1;
        transform: translateX(0);
    }
}

/* Staggered animation delays */
.input-group:nth-child(1) .input-wrapper {
    animation-delay: 0s;
}

.input-group:nth-child(2) .input-wrapper {
    animation-delay: 0.1s;
}

.input-group:nth-child(3) .input-wrapper {
    animation-delay: 0.2s;
}

.input-icon {
    position: absolute;
    left: 18px;
    color: #9ca3af;
    font-size: 16px;
    z-index: 1;
    transition: all 0.3s ease;
}

.form-input:focus~.input-icon {
    color: #667eea;
    transform: scale(1.1);
}

.form-input {
    width: 100%;
    padding: 18px 18px 18px 52px;
    border: 2px solid #e5e7eb;
    border-radius: 16px;
    font-size: 15px;
    transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    background: #fff;
    color: #374151;
    box-sizing: border-box;
    position: relative;
}

.form-input:focus {
    outline: none;
    border-color: #667eea;
    box-shadow:
        0 0 0 4px rgba(102, 126, 234, 0.1),
        0 8px 25px rgba(102, 126, 234, 0.15);
    transform: translateY(-2px);
}

.form-input::placeholder {
    color: #9ca3af;
}

.password-toggle {
    position: absolute;
    right: 16px;
    background: none;
    border: none;
    color: #9ca3af;
    cursor: pointer;
    font-size: 16px;
    padding: 4px;
    border-radius: 4px;
    transition: color 0.3s ease;
}

.password-toggle:hover {
    color: #667eea;
}

.forgot-password {
    text-align: right;
    margin-bottom: 24px;
}

.forgot-link {
    color: #667eea;
    text-decoration: none;
    font-size: 14px;
    font-weight: 500;
    transition: color 0.3s ease;
}

.forgot-link:hover {
    color: #5a67d8;
    text-decoration: underline;
}

/* ============================================================
   SUBMIT BUTTON & LOADING
   ============================================================ */
.submit-button {
    width: 100%;
    padding: 18px 20px;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    color: white;
    border: none;
    border-radius: 16px;
    font-size: 16px;
    font-weight: 600;
    cursor: pointer;
    transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    display: flex;
    align-items: center;
    justify-content: center;
    min-height: 56px;
    position: relative;
    overflow: hidden;
}

.submit-button::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
    transition: left 0.6s ease;
}

.submit-button:hover::before {
    left: 100%;
}

.submit-button:hover:not(:disabled) {
    transform: translateY(-3px) scale(1.02);
    box-shadow:
        0 12px 30px rgba(102, 126, 234, 0.4),
        0 0 0 1px rgba(255, 255, 255, 0.2);
}

.submit-button:active:not(:disabled) {
    transform: translateY(-1px) scale(0.98);
    transition: all 0.1s ease;
}

.submit-button:disabled {
    opacity: 0.7;
    cursor: not-allowed;
    transform: none;
}

.loading-spinner {
    width: 20px;
    height: 20px;
    border: 2px solid rgba(255, 255, 255, 0.3);
    border-top: 2px solid white;
    border-radius: 50%;
    animation: spin 1s linear infinite;
}

@keyframes spin {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

/* ============================================================
   TOGGLE MODE SECTION
   ============================================================ */
.toggle-mode {
    text-align: center;
    margin-top: 24px;
}

.toggle-mode p {
    color: #718096;
    font-size: 14px;
    margin: 0;
}

.toggle-button {
    background: none;
    border: none;
    color: #667eea;
    font-weight: 600;
    cursor: pointer;
    font-size: 14px;
    text-decoration: underline;
    transition: color 0.3s ease;
}

.toggle-button:hover {
    color: #5a67d8;
}

/* ====================================================================
   RESPONSIVE DESIGN - MOBILE OPTIMIZATIONS
   ==================================================================== */
@media (max-width: 480px) {
    .login-container {
        padding: 16px;
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
        animation: mobileBackgroundShift 15s ease-in-out infinite;
    }

    @keyframes mobileBackgroundShift {

        0%,
        100% {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
        }

        25% {
            background: linear-gradient(135deg, #764ba2 0%, #667eea 100%);
        }

        50% {
            background: linear-gradient(135deg, #667eea 0%, #9f7aea 100%);
        }

        75% {
            background: linear-gradient(135deg, #9f7aea 0%, #667eea 100%);
        }
    }

    /* Mobile Card Adjustments */
    .login-card {
        padding: 0;
        border-radius: 20px;
        animation: slideUp 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
        box-shadow:
            0 30px 60px rgba(0, 0, 0, 0.2),
            0 0 0 1px rgba(255, 255, 255, 0.3),
            inset 0 1px 0 rgba(255, 255, 255, 0.4);
    }

    /* Mobile Sigmoid Curve */
    .login-card::before {
        height: 240px;
        background: linear-gradient(135deg, #667eea 0%, #764ba2 50%, #9f7aea 100%);
        clip-path: polygon(0 0,
                100% 0,
                100% 35%,
                90% 40%,
                75% 50%,
                60% 58%,
                50% 60%,
                40% 58%,
                25% 50%,
                10% 40%,
                0 35%);
    }

    /* Mobile App Icon */
    .app-icon-container {
        top: 55px;
        width: 75px;
        height: 75px;
    }

    .app-icon {
        width: 50px;
        height: 50px;
        font-size: 22px;
    }

    /* Mobile Content Padding */
    .login-content {
        padding: 160px 24px 32px 24px;
    }

    /* Mobile Title Animations */
    .login-title {
        font-size: 28px;
        animation:
            titleGlow 2s ease-in-out infinite alternate,
            mobileTitlePulse 4s ease-in-out infinite;
    }

    @keyframes mobileTitlePulse {

        0%,
        100% {
            transform: scale(1);
        }

        50% {
            transform: scale(1.05);
        }
    }

    /* Mobile Social Buttons */
    .social-button {
        padding: 16px 16px;
        font-size: 14px;
        border-radius: 18px;
        position: relative;
        transform-style: preserve-3d;
    }

    .social-button:hover {
        transform: translateY(-4px) scale(1.03) rotateX(5deg);
        box-shadow:
            0 15px 30px rgba(0, 0, 0, 0.25),
            0 0 20px rgba(102, 126, 234, 0.3);
    }

    .social-button:active {
        transform: translateY(-1px) scale(0.97) rotateX(-2deg);
        animation: mobileButtonRipple 0.6s ease-out;
    }

    @keyframes mobileButtonRipple {
        0% {
            box-shadow: 0 0 0 0 rgba(102, 126, 234, 0.7);
        }

        70% {
            box-shadow: 0 0 0 20px rgba(102, 126, 234, 0);
        }

        100% {
            box-shadow: 0 0 0 0 rgba(102, 126, 234, 0);
        }
    }

    /* Mobile Form Inputs */
    .form-input {
        padding: 16px 16px 16px 48px;
        font-size: 14px;
        border-radius: 18px;
        transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    }

    .form-input:focus {
        transform: translateY(-3px) scale(1.02);
        box-shadow:
            0 0 0 4px rgba(102, 126, 234, 0.15),
            0 12px 30px rgba(102, 126, 234, 0.2),
            0 0 20px rgba(102, 126, 234, 0.1);
    }

    .input-wrapper {
        animation:
            inputSlideIn 0.6s ease-out,
            mobileInputFloat 8s ease-in-out infinite;
    }

    @keyframes mobileInputFloat {

        0%,
        100% {
            transform: translateX(0px);
        }

        25% {
            transform: translateX(2px);
        }

        75% {
            transform: translateX(-2px);
        }
    }

    /* Mobile Submit Button */
    .submit-button {
        padding: 18px 16px;
        font-size: 15px;
        border-radius: 18px;
        position: relative;
        overflow: hidden;
    }

    .submit-button:hover:not(:disabled) {
        transform: translateY(-4px) scale(1.03);
        box-shadow:
            0 15px 35px rgba(102, 126, 234, 0.5),
            0 0 25px rgba(102, 126, 234, 0.3),
            0 0 0 1px rgba(255, 255, 255, 0.3);
        animation: mobileButtonGlow 2s ease-in-out infinite;
    }

    @keyframes mobileButtonGlow {

        0%,
        100% {
            box-shadow:
                0 15px 35px rgba(102, 126, 234, 0.5),
                0 0 25px rgba(102, 126, 234, 0.3),
                0 0 0 1px rgba(255, 255, 255, 0.3);
        }

        50% {
            box-shadow:
                0 15px 35px rgba(102, 126, 234, 0.7),
                0 0 35px rgba(102, 126, 234, 0.5),
                0 0 0 1px rgba(255, 255, 255, 0.4);
        }
    }

    /* Mobile Divider Animation */
    .divider {
        margin: 28px 0;
        animation: dividerSlide 1s ease-out;
    }

    @keyframes dividerSlide {
        from {
            opacity: 0;
            transform: scaleX(0);
        }

        to {
            opacity: 1;
            transform: scaleX(1);
        }
    }

    /* Mobile Error Message */
    .error-message {
        animation:
            shake 0.5s ease-in-out,
            mobileErrorPulse 2s ease-in-out infinite;
        border-radius: 12px;
    }

    @keyframes mobileErrorPulse {

        0%,
        100% {
            background: #fed7d7;
            box-shadow: 0 0 0 0 rgba(197, 48, 48, 0.3);
        }

        50% {
            background: #fecaca;
            box-shadow: 0 0 0 8px rgba(197, 48, 48, 0);
        }
    }

    /* Mobile Toggle Button */
    .toggle-button {
        transition: all 0.3s ease;
        position: relative;
    }

    .toggle-button:hover {
        transform: scale(1.05);
        color: #5a67d8;
        text-shadow: 0 0 8px rgba(102, 126, 234, 0.5);
    }
}

/* ==============================================
   Dark Mode Support
   ============================================== */
@media (prefers-color-scheme: dark) {
    .login-container {
        background: linear-gradient(135deg, #1a202c 0%, #2d3748 100%);
    }

    .login-card {
        background: rgba(26, 32, 44, 0.95);
        border: 1px solid rgba(255, 255, 255, 0.1);
    }

    .login-title {
        color: #f7fafc;
    }

    .login-subtitle {
        color: #a0aec0;
    }

    .form-input {
        background: #2d3748;
        border-color: #4a5568;
        color: #f7fafc;
    }

    .form-input:focus {
        border-color: #667eea;
    }

    .google-button {
        background: #2d3748;
        color: #f7fafc;
        border-color: #4a5568;
    }

    .google-button:hover:not(:disabled) {
        background: #4a5568;
    }
}

/* Advanced Mobile Micro-Animations */
@media (max-width: 768px) {

    /* Touch ripple effect */
    .touch-ripple {
        position: relative;
        overflow: hidden;
    }

    .touch-ripple::before {
        content: '';
        position: absolute;
        top: 50%;
        left: 50%;
        width: 0;
        height: 0;
        border-radius: 50%;
        background: rgba(255, 255, 255, 0.3);
        transform: translate(-50%, -50%);
        transition: width 0.6s, height 0.6s;
        pointer-events: none;
    }

    .touch-ripple:active::before {
        width: 200px;
        height: 200px;
    }

    /* Enhanced input focus animations */
    .input-wrapper:focus-within {
        animation: inputFocusGlow 0.3s ease-out;
    }

    @keyframes inputFocusGlow {
        0% {
            transform: scale(1);
        }

        50% {
            transform: scale(1.02);
        }

        100% {
            transform: scale(1);
        }
    }

    /* Staggered animation for form elements */
    .input-group {
        opacity: 0;
        animation: staggeredFadeIn 0.6s ease-out forwards;
    }

    .input-group:nth-child(1) {
        animation-delay: 0.1s;
    }

    .input-group:nth-child(2) {
        animation-delay: 0.2s;
    }

    .input-group:nth-child(3) {
        animation-delay: 0.3s;
    }

    @keyframes staggeredFadeIn {
        from {
            opacity: 0;
            transform: translateY(20px) scale(0.95);
        }

        to {
            opacity: 1;
            transform: translateY(0) scale(1);
        }
    }

    /* Enhanced password toggle animation */
    .password-toggle {
        transition: all 0.3s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    }

    .password-toggle:active {
        transform: scale(0.9) rotate(10deg);
    }

    /* Mobile-specific hover states */
    @media (hover: none) and (pointer: coarse) {
        .social-button:hover {
            transform: none;
            box-shadow: none;
        }

        .submit-button:hover {
            transform: none;
            box-shadow: none;
        }

        .form-input:hover {
            transform: none;
        }
    }

    /* Loading animation enhancement for mobile */
    .loading-spinner {
        width: 24px;
        height: 24px;
        border: 3px solid rgba(255, 255, 255, 0.3);
        border-top: 3px solid white;
        border-radius: 50%;
        animation: spin 1s linear infinite, loadingPulse 2s ease-in-out infinite;
    }

    @keyframes loadingPulse {

        0%,
        100% {
            transform: scale(1);
        }

        50% {
            transform: scale(1.1);
        }
    }

    /* Enhanced error message for mobile */
    .error-message {
        position: relative;
        overflow: hidden;
    }

    .error-message::before {
        content: '';
        position: absolute;
        top: 0;
        left: -100%;
        width: 100%;
        height: 100%;
        background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);
        animation: errorShimmer 2s ease-in-out infinite;
    }

    @keyframes errorShimmer {
        0% {
            left: -100%;
        }

        100% {
            left: 100%;
        }
    }

    /* Card entrance animation */
    .login-card {
        animation: slideUp 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    }

    .login-card::after {
        content: '';
        position: absolute;
        top: -50%;
        left: -50%;
        width: 200%;
        height: 200%;
        background:
            radial-gradient(circle at 30% 30%, rgba(255, 255, 255, 0.1) 0%, transparent 50%),
            radial-gradient(circle at 70% 70%, rgba(102, 126, 234, 0.1) 0%, transparent 50%);
        animation: mobileCardShimmer 8s ease-in-out infinite;
        pointer-events: none;
        z-index: 0;
    }

    @keyframes mobileCardShimmer {

        0%,
        100% {
            transform: rotate(0deg) scale(1);
            opacity: 0.3;
        }

        50% {
            transform: rotate(180deg) scale(1.1);
            opacity: 0.6;
        }
    }

    /* Toggle mode animation */
    .toggle-mode {
        animation: fadeInUp 0.6s ease-out 0.5s both;
    }

    @keyframes fadeInUp {
        from {
            opacity: 0;
            transform: translateY(20px);
        }

        to {
            opacity: 1;
            transform: translateY(0);
        }
    }

    /* Social icons rotation on interaction */
    .social-icon {
        transition: transform 0.3s ease;
    }

    .social-button:active .social-icon {
        transform: rotate(360deg) scale(1.1);
    }
}